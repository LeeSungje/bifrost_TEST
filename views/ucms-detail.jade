extends layout

block content
   input(type="hidden", val='UCMS')#inputCurrentSystem
   .ucms-container
     .col-sm-12.ucms-detail-panel
       .col-md-offset-4.col-md-2.ucms-detail-header
         .ucms-detail-ttl UCMS
         span#locationContainer
       .col-md-1.ucms-detail-header
         canvas.ucms-chart
         span#curUcmsTpsContainer
         span.ucms-txt-delimiter &nbsp/&nbsp
         span#totUcmsTpsContainer
         span.ucms-txt-delimiter Tps
       .col-sm-offset-3.col-sm-1.ucms-rm
         img(src='/img/title/system_solution.png', alt='', width="80" height="80").a(onclick='location.href="/ucms-rm"') 
         .ucms-txt-delimiter 극단적RM 
     
     .col-sm-12.ucms-detail-panel    
       .col-sm-5.skt-panel.ucms-detail-panel.stat-panel
         .col-sm-12
           img(src='/img/title/system_stat.png', alt='', width="40" height="40")
           span.ucms-detail-txt 통계
              
       .col-sm-5.skt-panel.ucms-detail-panel.alarm-panel
         .col-sm-12
           img(src='/img/title/system_alarm.png', alt='', width="40" height="40")
           span.ucms-detail-txt 알람           

   
   
block javascripts
  script(src="/js/flot/jquery.flot.time.js")
  script(src="/js/twinkle/jquery.twinkle-0.9.0.min.js")
  script(src="/js/skt/ucms-detail.js")
  script(src="/js/charts/Chart.js")
  script(src="/js/utils.js")    